{
  "name": "pump-dex-mini-app",
  "version": "1.0.0",
  "description": "Telegram Mini App for Solana token trading analysis with real-time data from PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "bot": "node bot.js",
    "dev:bot": "nodemon bot.js",
    "build": "echo 'Static files are already built'",
    "test": "echo 'No tests specified yet'",
    "lint": "eslint *.js",
    "postinstall": "echo 'Installation completed. See README.md for setup instructions.'"
  },
  "keywords": [
    "telegram",
    "mini-app",
    "solana",
    "defi",
    "trading",
    "cryptocurrency",
    "blockchain",
    "pump-fun",
    "postgresql"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://github.com/yourusername"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/pump-dex-mini-app.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/pump-dex-mini-app/issues"
  },
  "homepage": "https://github.com/yourusername/pump-dex-mini-app#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "telegraf": "^4.16.3",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0"
  },
  "nodemonConfig": {
    "watch": [
      "*.js",
      "public/**/*"
    ],
    "ignore": [
      "node_modules/**/*",
      ".git/**/*"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  }
}